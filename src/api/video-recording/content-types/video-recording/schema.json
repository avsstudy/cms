{
  "kind": "collectionType",
  "collectionName": "video_recordings",
  "info": {
    "singularName": "video-recording",
    "pluralName": "video-recordings",
    "displayName": "Video_Recording"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "card_cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "video_type": {
      "type": "enumeration",
      "required": true,
      "default": "webinar_recording",
      "enum": [
        "webinar_recording",
        "session_recording",
        "mini_course_recording",
        "upcoming_session",
        "upcoming_mini_course",
        "video_review"
      ]
    },
    "stream_date": {
      "type": "date"
    },
    "speaker": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::speaker.speaker",
      "inversedBy": "video_recording"
    },
    "top": {
      "type": "boolean",
      "required": false,
      "default": false
    },
    "subscription_type": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subscription-type.subscription-type",
      "mappedBy": "video_recording"
    },
    "general_content": {
      "type": "dynamiczone",
      "components": [
        "shared.video-review",
        "shared.mini-course-lesson",
        "shared.mini-course-landing",
        "shared.custom-video-recording",
        "shared.upcoming-session"
      ]
    }
  }
}
