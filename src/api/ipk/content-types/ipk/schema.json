{
  "kind": "collectionType",
  "collectionName": "ipks",
  "info": {
    "singularName": "ipk",
    "pluralName": "ipks",
    "displayName": "IPK"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ipk_title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "ipk_title",
      "required": true
    },
    "ipk_date": {
      "type": "date",
      "required": true
    },
    "ipk_file": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "files"
      ]
    },
    "topic": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::topic.topic",
      "inversedBy": "ipks"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "ipks"
    },
    "views": {
      "type": "integer",
      "default": 1
    },
    "topic_dps": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::topic-dps.topic-dps",
      "inversedBy": "ipk"
    },
    "subscriptions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subscription.subscription",
      "inversedBy": "ipks"
    }
  }
}
