{
  "kind": "collectionType",
  "collectionName": "handbooks",
  "info": {
    "singularName": "handbook",
    "pluralName": "handbooks",
    "displayName": "Handbook"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "topic": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::topic.topic",
      "inversedBy": "handbooks"
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "handbooks"
    },
    "views": {
      "type": "integer",
      "default": 1
    },
    "subscription_type": {
      "type": "enumeration",
      "required": true,
      "default": "free",
      "enum": [
        "free",
        "paid"
      ]
    },
    "pinned": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "general_content": {
      "type": "dynamiczone",
      "components": [
        "shared.zakon",
        "shared.text",
        "shared.image",
        "shared.custom-table",
        "shared.custom-quote"
      ],
      "required": true
    },
    "subscriptions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subscription.subscription",
      "inversedBy": "handbooks"
    }
  }
}
