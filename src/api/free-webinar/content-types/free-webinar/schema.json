{
  "kind": "collectionType",
  "collectionName": "free_webinars",
  "info": {
    "singularName": "free-webinar",
    "pluralName": "free-webinars",
    "displayName": "Free_webinar"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "card_cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "webinar_type": {
      "type": "enumeration",
      "required": true,
      "default": "single",
      "enum": [
        "single",
        "series"
      ]
    },
    "speaker": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::speaker.speaker",
      "inversedBy": "free_webinar"
    },
    "date_1": {
      "type": "date"
    },
    "date_2": {
      "type": "date"
    },
    "date_3": {
      "type": "date"
    },
    "time": {
      "type": "time",
      "required": true
    },
    "stream_url": {
      "type": "string",
      "required": true
    },
    "pinned": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "program_content_1": {
      "type": "component",
      "component": "shared.text",
      "repeatable": false,
      "required": true
    },
    "program_content_2": {
      "type": "component",
      "component": "shared.text",
      "repeatable": false,
      "required": true
    },
    "program_content_3": {
      "type": "component",
      "component": "shared.text",
      "repeatable": false,
      "required": true
    }
  }
}
