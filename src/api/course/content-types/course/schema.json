{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "client_title": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "card_cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "course_type": {
      "type": "enumeration",
      "required": true,
      "default": "course_recording",
      "enum": [
        "course_recording",
        "course_online"
      ]
    },
    "speaker": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::speaker.speaker",
      "inversedBy": "courses"
    },
    "subscription_type": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subscription-type.subscription-type",
      "inversedBy": "courses"
    },
    "category": {
      "type": "enumeration",
      "required": true,
      "default": "industry",
      "enum": [
        "industry",
        "basics",
        "automation",
        "career"
      ]
    },
    "topic": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::topic.topic",
      "inversedBy": "courses"
    },
    "general_content": {
      "type": "dynamiczone",
      "components": [
        "shared.course-online-landing",
        "shared.course-recording-landing",
        "shared.course-subscription-landing"
      ]
    },
    "reviews": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::review.review",
      "inversedBy": "courses"
    },
    "study_session": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::study-session.study-session",
      "inversedBy": "course"
    },
    "course_accesses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-access.course-access",
      "mappedBy": "course"
    },
    "course_test": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::session-test.session-test",
      "inversedBy": "course"
    },
    "subscription": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subscription.subscription",
      "inversedBy": "course"
    }
  }
}
