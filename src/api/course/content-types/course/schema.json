{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "card_cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "course_type": {
      "type": "enumeration",
      "required": true,
      "default": "course_recording",
      "enum": [
        "course_recording",
        "course_online"
      ]
    },
    "speaker": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::speaker.speaker",
      "inversedBy": "courses"
    },
    "subscription_type": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subscription-type.subscription-type",
      "inversedBy": "courses"
    },
    "category": {
      "type": "enumeration",
      "required": true,
      "default": "business",
      "enum": [
        "business",
        "finance"
      ]
    },
    "topic": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::topic.topic",
      "inversedBy": "courses"
    }
  }
}
