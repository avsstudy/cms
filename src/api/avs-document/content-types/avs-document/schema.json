{
  "kind": "collectionType",
  "collectionName": "avs_documents",
  "info": {
    "singularName": "avs-document",
    "pluralName": "avs-documents",
    "displayName": "AVS_Document"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "topic": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::topic.topic",
      "inversedBy": "avs_documents"
    },
    "author": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "avs_documents"
    },
    "views": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "subscription_type": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subscription-type.subscription-type",
      "inversedBy": "avs_documents"
    },
    "pinned": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "general_content_blank": {
      "type": "dynamiczone",
      "components": [
        "shared.blank"
      ],
      "required": true
    },
    "general_content_zrazok": {
      "type": "dynamiczone",
      "components": [
        "shared.zrazok"
      ]
    },
    "general_content_useful_files": {
      "type": "dynamiczone",
      "components": [
        "shared.useful-files"
      ]
    }
  }
}
