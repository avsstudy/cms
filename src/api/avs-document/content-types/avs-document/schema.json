{
  "kind": "collectionType",
  "collectionName": "avs_documents",
  "info": {
    "singularName": "avs-document",
    "pluralName": "avs-documents",
    "displayName": "AVS_Document"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "document_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::document-category.document-category",
      "inversedBy": "avs_document"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "topic": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::topic.topic",
      "inversedBy": "avs_documents"
    },
    "author": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "avs_documents"
    },
    "views": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "pinned": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "subscriptions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subscription.subscription",
      "inversedBy": "avs_documents"
    },
    "general_content": {
      "type": "dynamiczone",
      "components": [
        "shared.custom-document",
        "shared.npa"
      ]
    }
  }
}
