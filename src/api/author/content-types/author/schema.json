{
  "kind": "collectionType",
  "collectionName": "authors",
  "info": {
    "singularName": "author",
    "pluralName": "authors",
    "displayName": "Author"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "articles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
      "mappedBy": "author"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "handbooks": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::handbook.handbook",
      "mappedBy": "authors"
    },
    "ipks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::ipk.ipk",
      "mappedBy": "author"
    },
    "avs_documents": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::avs-document.avs-document",
      "mappedBy": "author"
    },
    "expert_answer": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::expert-answer.expert-answer",
      "mappedBy": "author"
    }
  }
}
