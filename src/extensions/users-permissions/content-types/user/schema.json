{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "phone": {
      "type": "string",
      "required": true,
      "regex": "^\\+380[0-9()\\s-]{7,}$"
    },
    "user_questions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::user-question.user-question",
      "mappedBy": "user"
    },
    "course_accesses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::course-access.course-access",
      "mappedBy": "user"
    },
    "session_progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::session-progress.session-progress",
      "mappedBy": "user"
    },
    "homework_progresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::homework-progress.homework-progress",
      "mappedBy": "user"
    },
    "test_attempts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::test-attempt.test-attempt",
      "mappedBy": "user"
    },
    "session_questions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::session-question.session-question",
      "mappedBy": "users_permissions_user"
    },
    "zoho_id": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female"
      ]
    },
    "birthday": {
      "type": "date"
    },
    "photo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    }
  }
}